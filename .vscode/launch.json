  "version": "0.2.0",
  "configurations": [
    {
      "name": "UI",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/ui"
    },
    {
      "name": "API",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outputCapture": "std"
    }
  ],
  "compounds": [
    {
      "name": "Run Project",
      "configurations": ["UI", "API"]
    }
  ]
}
