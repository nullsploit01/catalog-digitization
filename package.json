{
  "name": "catalog-digitization",
  "version": "0.0.0",
  "description": "Build For Bharat Hackathon Project",
  "main": "index.ts",
  "scripts": {
    "test": "jest",
    "build": "rimraf ./dist && gulp",
    "start": "cd dist && node index.js",
    "start:gulp": "npm-run-all build start",
    "start:build": "npm run build && npm run start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start:gulp",
    "prettier:fix": "prettier --write ./src/**/*{js,ts}",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullsploit01/catalog-digitization.git"
  },
  "author": "nullsploit01",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nullsploit01/catalog-digitization/issues"
  },
  "homepage": "https://github.com/nullsploit01/catalog-digitization#readme",
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/gulp": "^4.0.17",
    "@types/node": "^20.10.5",
    "del": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "npm run prettier:fix"
    ]
  }
